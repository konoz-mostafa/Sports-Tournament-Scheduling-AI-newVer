    "        # Calculate metrics\n",
    "        initial_fitness = results['best_fitness_history'][0]\n",
    "        final_fitness = results['best_fitness']\n",
    "        final_avg_fitness = results['avg_fitness_history'][-1]\n",
    "        num_generations = results['final_generation']\n",
    "        improvement_pct = ((final_fitness - initial_fitness) / initial_fitness * 100) if initial_fitness > 0 else 0\n",
    "        \n",
    "        # Store experiment data\n",
    "        experiment_data = {\n",
    "            'config': config.copy(),\n",
    "            'results': results,\n",
    "            'metrics': {\n",
    "                'initial_fitness': initial_fitness,\n",
    "                'final_fitness': final_fitness,\n",
    "                'final_avg_fitness': final_avg_fitness,\n",
    "                'num_generations': num_generations,\n",
    "                'improvement_pct': improvement_pct\n",
    "            }\n",
    "        }\n",
    "        \n",
    "        experiments_results.append(experiment_data)\n",
    "        \n",
    "        print(f\"\\n‚úÖ Experiment {i} completed:\")\n",
    "        print(f\"   Best Fitness: {final_fitness:.6f}\")\n",
    "        print(f\"   Final Avg Fitness: {final_avg_fitness:.6f}\")\n",
    "        print(f\"   Generations: {num_generations}\")\n",
    "        print(f\"   Improvement: {improvement_pct:.2f}%\")\n",
    "    \n",
    "    print(f\"\\n{'='*70}\")\n",
    "    print(f\"‚úÖ ALL EXPERIMENTS COMPLETED!\")\n",
    "    print(f\"   Total experiments: {len(experiments_results)}\")\n",
    "    print(f\"{'='*70}\\n\")\n",
    "    \n",
    "    return experiments_results\n",
    "\n",
    "\n",
    "def plot_results(experiments_data):\n",
    "    \"\"\"\n",
    "    Plot results for all experiments.\n",
    "    \n",
    "    Generates:\n",
    "    1. Individual plot for each experiment\n",
    "    2. Comparison plot showing all experiments together\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    experiments_data : list - List of experiment data dictionaries\n",
    "    \"\"\"\n",
    "    if len(experiments_data) == 0:\n",
    "        print(\"No experiments to plot.\")\n",
    "        return\n",
    "    \n",
    "    # ===== INDIVIDUAL PLOTS =====\n",
    "    print(\"\\nüìä Generating individual experiment plots...\\n\")\n",
    "    for i, exp_data in enumerate(experiments_data, 1):\n",
    "        results = exp_data['results']\n",
    "        config = exp_data['config']\n",
    "        metrics = exp_data['metrics']\n",
    "        \n",
    "        plt.figure(figsize=(12, 6))\n",
    "        \n",
    "        generations = range(len(results['best_fitness_history']))\n",
    "        \n",
    "        # Plot best and average fitness\n",
    "        plt.plot(generations, results['best_fitness_history'], \n",
    "                'b-', linewidth=2.5, label='Best Fitness', marker='o', markersize=4)\n",
    "        plt.plot(generations, results['avg_fitness_history'], \n",
    "                'g--', linewidth=2, label='Average Fitness', marker='s', markersize=3)\n",
    "        \n",
    "        # Title with configuration\n",
    "        title = f\"Experiment {i} - Fitness Evolution\\n\"\n",
    "        title += f\"Crossover: {config['crossover_type']}, \"\n",
    "        title += f\"MutRate: {config['mutation_rate']}, \"\n",
    "        title += f\"PopSize: {config['pop_size']}, \"\n",
    "        title += f\"Generations: {config['generations']}\"\n",
    "        \n",
    "        plt.xlabel('Generation', fontsize=12, fontweight='bold')\n",
    "        plt.ylabel('Fitness Value', fontsize=12, fontweight='bold')\n",
    "        plt.title(title, fontsize=11, fontweight='bold')\n",
    "        plt.legend(fontsize=10)\n",
    "        plt.grid(True, alpha=0.3, linestyle='--')\n",
    "        \n",
    "        # Add metrics text box\n",
    "        textstr = f\"Best Fitness: {metrics['final_fitness']:.6f}\\n\"\n",
    "        textstr += f\"Avg Fitness: {metrics['final_avg_fitness']:.6f}\\n\"\n",
    "        textstr += f\"Generations: {metrics['num_generations']}\\n\"\n",
    "        textstr += f\"Improvement: {metrics['improvement_pct']:.2f}%\"\n",
    "        plt.text(0.02, 0.98, textstr, transform=plt.gca().transAxes,\n",
    "                fontsize=9, verticalalignment='top',\n",
    "                bbox=dict(boxstyle='round', facecolor='wheat', alpha=0.5))\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "    \n",
    "    # ===== COMPARISON PLOT =====\n",
    "    print(\"\\nüìà Generating comparison plot...\\n\")\n",
    "    plt.figure(figsize=(14, 8))\n",
    "    \n",
    "    # Color palette for different experiments\n",
    "    colors = plt.cm.tab10(np.linspace(0, 1, len(experiments_data)))\n",
    "    \n",
    "    for i, exp_data in enumerate(experiments_data):\n",
    "        results = exp_data['results']\n",
    "        config = exp_data['config']\n",
    "        \n",
    "        # Create label with key parameters\n",
    "        label = f\"Exp {i+1}: \"\n",
    "        label += f\"Cross={config['crossover_type']}, \"\n",
    "        label += f\"Mut={config['mutation_rate']}, \"\n",
    "        label += f\"Pop={config['pop_size']}, \"\n",
    "        label += f\"Gen={config['generations']}\"\n",
    "        \n",
    "        generations = range(len(results['best_fitness_history']))\n",
    "        plt.plot(generations, results['best_fitness_history'], \n",
    "                linewidth=2.5, label=label, marker='o', markersize=3, color=colors[i])\n",
    "    \n",
    "    plt.xlabel('Generation', fontsize=13, fontweight='bold')\n",
    "    plt.ylabel('Best Fitness', fontsize=13, fontweight='bold')\n",
    "    plt.title('Comparative Analysis: Best Fitness Evolution Across All Experiments', \n",
    "             fontsize=14, fontweight='bold')\n",
    "    plt.legend(fontsize=9, loc='best', ncol=1)\n",
    "    plt.grid(True, alpha=0.3, linestyle='--')\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "\n",
    "def display_summary_table(experiments_data):\n",
    "    \"\"\"\n",
    "    Display a summary table of all experiments.\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    experiments_data : list - List of experiment data dictionaries\n",
    "    \"\"\"\n",
    "    if len(experiments_data) == 0:\n",
    "        print(\"No experiments to summarize.\")\n",
    "        return\n",
    "    \n",
    "    # Prepare data for DataFrame\n",
    "    summary_data = []\n",
    "    for i, exp_data in enumerate(experiments_data, 1):\n",
    "        config = exp_data['config']\n",
    "        metrics = exp_data['metrics']\n",
    "        \n",
    "        summary_data.append({\n",
    "            'Experiment': i,\n",
    "            'Crossover Type': config['crossover_type'],\n",
    "            'Mutation Rate': config['mutation_rate'],\n",
    "            'Population Size': config['pop_size'],\n",
    "            'Generations': config['generations'],\n",
    "            'Best Fitness': metrics['final_fitness'],\n",
    "            'Final Avg Fitness': metrics['final_avg_fitness'],\n",
    "            'Improvement %': metrics['improvement_pct']\n",
    "        })\n",
    "    \n",
    "    # Create and display DataFrame\n",
    "    df = pd.DataFrame(summary_data)\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*100)\n",
    "    print(\"üìä EXPERIMENT SUMMARY TABLE\")\n",
    "    print(\"=\"*100)\n",
    "    print(df.to_string(index=False))\n",
    "    print(\"=\"*100 + \"\\n\")\n",
    "    \n",
    "    return df\n",
    "\n",
    "\n",
    "# ==================== IPYWIDGETS GUI ====================\n",
    "\n",
    "def create_experiments_gui(teams, venues, dates, match_times):\n",
    "    \"\"\"\n",
    "    Create the Experiments GUI using ipywidgets.\n",
    "    \n",
    "    Parameters:\n",
    "    -----------\n",
    "    teams, venues, dates, match_times : Tournament data\n",
    "    \n",
    "    Returns:\n",
    "    --------\n",
    "    widgets.VBox - The complete GUI widget\n",
    "    \"\"\"\n",
    "    # Output widget for displaying results\n",
    "    output = Output()\n",
    "    \n",
    "    # ===== PARAMETER WIDGETS =====\n",
    "    \n",
    "    # Crossover type dropdown\n",
    "    crossover_widget = widgets.Dropdown(\n",
    "        options=['order', 'two_point_order', 'uniform_order'],\n",
    "        value='order',\n",
    "        description='Crossover Type:',\n",
    "        style={'description_width': 'initial'},\n",
    "        layout=Layout(width='400px')\n",
    "    )\n",
    "    \n",
    "    # Mutation rate dropdown\n",
    "    mutation_rate_widget = widgets.Dropdown(\n",
    "        options=[0.05, 0.1, 0.15, 0.2],\n",
    "        value=0.1,\n",
    "        description='Mutation Rate:',\n",
    "        style={'description_width': 'initial'},\n",
    "        layout=Layout(width='400px')\n",
    "    )\n",
    "    \n",
    "    # Population size dropdown\n",
    "    pop_size_widget = widgets.Dropdown(\n",
    "        options=[20, 30, 50, 100],\n",
    "        value=50,\n",
    "        description='Population Size:',\n",
    "        style={'description_width': 'initial'},\n",
    "        layout=Layout(width='400px')\n",
    "    )\n",
    "    \n",
    "    # Number of generations dropdown\n",
    "    generations_widget = widgets.Dropdown(\n",
    "        options=[50, 100, 150, 200],\n",
    "        value=100,\n",
    "        description='Generations:',\n",
    "        style={'description_width': 'initial'},\n",
    "        layout=Layout(width='400px')\n",
    "    )\n",
    "    \n",
    "    # Number of experiments to run\n",
    "    num_experiments_widget = widgets.IntSlider(\n",
    "        value=3,\n",
    "        min=1,\n",
    "        max=5,\n",
    "        step=1,\n",
    "        description='Number of Experiments:',\n",
    "        style={'description_width': 'initial'},\n",
    "        layout=Layout(width='400px')\n",
    "    )\n",
    "    \n",
    "    # ===== BUTTONS =====\n",
    "    \n",
    "    run_button = widgets.Button(\n",
    "        description='üöÄ Run Experiments',\n",
    "        button_style='success',\n",
    "        layout=Layout(width='200px', height='40px')\n",
    "    )\n",
    "    \n",
    "    plot_button = widgets.Button(\n",
    "        description='üìä Plot Results',\n",
    "        button_style='info',\n",
    "        layout=Layout(width='200px', height='40px')\n",
    "    )\n",
    "    \n",
    "    table_button = widgets.Button(\n",
    "        description='üìã Show Summary Table',\n",
    "        button_style='warning',\n",
    "        layout=Layout(width='200px', height='40px')\n",
    "    )\n",
    "    \n",
    "    clear_button = widgets.Button(\n",
    "        description='üóëÔ∏è Clear Results',\n",
    "        button_style='danger',\n",
    "        layout=Layout(width='200px', height='40px')\n",
    "    )\n",
    "    \n",
    "    # ===== BUTTON HANDLERS =====\n",
    "    \n",
    "    def on_run_clicked(b):\n",
    "        \"\"\"Handle Run Experiments button click\"\"\"\n",
    "        with output:\n",
    "            clear_output(wait=True)\n",
    "            \n",
    "            # Get parameter values\n",
    "            crossover_type = crossover_widget.value\n",
    "            mutation_rate = mutation_rate_widget.value\n",
    "            pop_size = pop_size_widget.value\n",
    "            generations = generations_widget.value\n",
    "            num_experiments = num_experiments_widget.value\n",
    "            \n",
    "            # Generate configurations for multiple experiments\n",
    "            # Vary one parameter at a time for comparison\n",
    "            configs = []\n",
    "            \n",
    "            if num_experiments == 1:\n",
    "                # Single experiment with selected parameters\n",
    "                configs.append({\n",
    "                    'crossover_type': crossover_type,\n",
    "                    'mutation_rate': mutation_rate,\n",
    "                    'pop_size': pop_size,\n",
    "                    'generations': generations\n",
    "                })\n",
    "            else:\n",
    "                # Multiple experiments varying crossover type\n",
    "                crossover_options = ['order', 'two_point_order', 'uniform_order']\n",
    "                for i in range(min(num_experiments, len(crossover_options))):\n",
    "                    configs.append({\n",
    "                        'crossover_type': crossover_options[i],\n",
    "                        'mutation_rate': mutation_rate,\n",
    "                        'pop_size': pop_size,\n",
    "                        'generations': generations\n",
    "                    })\n",
    "            \n",
    "            # Run experiments\n",
    "            run_experiments(configs, teams, venues, dates, match_times)\n",
    "    \n",
    "    def on_plot_clicked(b):\n",
    "        \"\"\"Handle Plot Results button click\"\"\"\n",
    "        with output:\n",
    "            clear_output(wait=True)\n",
    "            if len(experiments_results) == 0:\n",
    "                print(\"No experiments to plot. Please run experiments first.\")\n",
    "            else:\n",
    "                plot_results(experiments_results)\n",
    "    \n",
    "    def on_table_clicked(b):\n",
    "        \"\"\"Handle Show Summary Table button click\"\"\"\n",
    "        with output:\n",
    "            clear_output(wait=True)\n",
    "            if len(experiments_results) == 0:\n",
    "                print(\"No experiments to summarize. Please run experiments first.\")\n",
    "            else:\n",
    "                display_summary_table(experiments_results)\n",
    "    \n",
    "    def on_clear_clicked(b):\n",
    "        \"\"\"Handle Clear Results button click\"\"\"\n",
    "        global experiments_results\n",
    "        experiments_results = []\n",
    "        with output:\n",
    "            clear_output(wait=True)\n",
    "            print(\"üóëÔ∏è All experiment results cleared.\")\n",
    "    \n",
    "    # Attach button handlers\n",
    "    run_button.on_click(on_run_clicked)\n",
    "    plot_button.on_click(on_plot_clicked)\n",
    "    table_button.on_click(on_table_clicked)\n",
    "    clear_button.on_click(on_clear_clicked)\n",
    "    \n",
    "    # ===== GUI LAYOUT =====\n",
    "    \n",
    "    parameters_box = widgets.VBox([\n",
    "        widgets.HTML(\"<h3>‚öôÔ∏è Experiment Parameters</h3>\"),\n",
    "        crossover_widget,\n",
    "        mutation_rate_widget,\n",
    "        pop_size_widget,\n",
    "        generations_widget,\n",
    "        num_experiments_widget\n",
    "    ], layout=Layout(padding='10px', border='2px solid #3498db'))\n",
    "    \n",
    "    buttons_box = widgets.HBox([\n",
    "        run_button,\n",
    "        plot_button,\n",
    "        table_button,\n",
    "        clear_button\n",
    "    ], layout=Layout(justify_content='center', margin='10px'))\n",
    "    \n",
    "    main_gui = widgets.VBox([\n",
    "        widgets.HTML(\"<h2>üî¨ Experiments & Comparison</h2>\"),\n",
    "        parameters_box,\n",
    "        buttons_box,\n",
    "        output\n",
    "    ])\n",
    "    \n",
    "    return main_gui\n",
    "\n",
    "\n",
    "print(\"‚úÖ Experiments GUI functions loaded!\")\n",
    "print(\"   Use: experiments_gui = create_experiments_gui(TEAMS, VENUES, DATES, MATCH_TIMES)\")\n",
    "print(\"   Then: display(experiments_gui)\")\n"
   ]
